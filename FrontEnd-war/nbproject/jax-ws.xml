<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="prTsWebService">
      <implementation-class>webservTotalOrder.prTsWebService</implementation-class>
    </service>
    <service name="heartBeatReceiverWebService">
      <implementation-class>heartBeatReceiver.heartBeatReceiverWebService</implementation-class>
    </service>
  </services>
  <clients>
    <client name="offerWebService">
      <wsdl-url>http://localhost:8080/ReplicaManager-war/offerWebService?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/ReplicaManager-war/offerWebService.wsdl</local-wsdl-file>
      <package-name>offer</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/ReplicaManager-war/offerWebService?wsdl</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
